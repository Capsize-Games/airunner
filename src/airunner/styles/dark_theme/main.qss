}

QComboBox {
    border-radius: None;
    padding: 5px;
    border: 1px solid @darkprimary-color;
    color: @light-color;
    background-color: @dark-color;
}

QComboBox::drop-down {
    border: none;
    background-color: @dark-color;
}

/* hover combobox */
QComboBox:hover {
    background-color: @darkerprimary-color;
}

/* the items in the QComboBox are not dark-color */
/* QComboBox QAbstractItemView {
    background-color: @dark-color;
    color: @light-color;
    border: 1px solid @darkprimary-color;
    selection-background-color: @primary-color;
    selection-color: @darkerprimary-color;
} */
/* hovering over items in the QComboBox should change the background color of those items */
QComboBox::item:hover QAbstractItemView {
    background-color: @darkerprimary-color;
}

/* hover over individual combobox items */
QComboBox::item:hover {
    background-color: @darkerprimary-color;
}

/* arrow */
QComboBox::down-arrow {
}

#mode_tab_widget QWidget {
    font-size: 10pt;
}

QFrame {
    border: none;
