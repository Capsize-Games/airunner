}

QGroupBox {
    padding: 10px;
}

QGroupBox QTabWidget QWidget {
    padding: 5px;
    border: 1px solid @darkerprimary-color;
}

QSlider {
    border: 1px solid @primary-color;
    padding-right: 0px;
    background-color: @darkerprimary-color;
}

QSlider::handle:horizontal {
    height: 20px;
    width: 25px;
    border: 1px solid @darkerprimary-color;
    border-radius: 0px;
    background-color: @darkerprimary-color;
}
QSlider::handle:horizontal:hover {
    background-color: @primary-color;
}
QSlider::groove:horizontal {
    height: 25px;
    background-color: transparent;
    border: transparent;
    border-radius: 0px;
}
QLabel#slider_label {
    font-size: 8pt;
    color: @light-color;
    font-weight: bold;
}
QDoubleSpinBox {
    background-color: @darkerprimary-color;
    border: 1px solid @primary-color;
    border-left: none;
}

