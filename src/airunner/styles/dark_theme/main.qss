QTableWidget {
    background-color: #ff0000;  /* Set the background color to match the dark theme */
    alternate-background-color: #222222;  /* Set the alternate background color */
    color: #ffffff;  /* Set the text color */
    gridline-color: #333333;  /* Set the grid line color */
}

QTableWidget::item {
    padding: 4px;
    border: none;
}

QTableWidget::item:selected {
    background-color: #444444;  /* Set the background color for selected items */
    color: #ffffff;
}


