}

QPushButton {
    border-radius: none;
    padding: 4px 4px 3px 4px;
    border: 1px solid @primary-color;
    color: @primary-color;
}

QPushButton:hover {
    border: 1px solid @darkprimary-color;
    background-color: @darkerprimary-color;
}

QPushButton:checked {
    border: 1px solid @primary-color;
}

QPushButton#prompt_builder_settings_button {
    border: none;
}

QProgressBar {
    border: 1px solid @primary-color;
