QDoubleSpinBox {
    background-color: #333; /* Set the desired background color */
    border: 1px solid rgba(0, 132, 185, 50); /* Add a border */
    padding: 2px; /* Adjust padding as needed */
    color: #C8C8C8; /* Set text color */
    min-width: 50px; /* Set the minimum width */
}

QDoubleSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right; /* Position the up arrow */
    border-left: 1px solid rgba(0, 132, 185, 50); /* Add a vertical line */
    border-bottom: 1px solid rgba(0, 132, 185, 50); /* Add a horizontal line */
    background-color: #111; /* Ensure the button inherits the background color */
}

QDoubleSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right; /* Position the down arrow */
    border-left: 1px solid rgba(0, 132, 185, 50); /* Add a vertical line */
    background-color: #111; /* Ensure the button inherits the background color */
}

#slider_widget QDoubleSpinBox::up-arrow {
    image: url(:/icons/up-arrow.svg); /* Ensure the up arrow is visible */
}

#slider_widget QDoubleSpinBox::down-arrow {
    image: url(:/icons/down-arrow.svg); /* Ensure the down arrow is visible */
}

#slider_widget QSlider {
