QSlider::groove:horizontal {
    background: #333; /* Set the desired gutter background color */
    height: 6px; /* Adjust the height as needed */
}

QSlider::groove:vertical {
    background: #333; /* Set the desired gutter background color */
    width: 6px; /* Adjust the width as needed */
}

QSlider::handle:horizontal {
    background: #C8C8C8; /* Set the desired handle color */
    border: 1px solid rgba(0, 132, 185, 255); /* Optional: add a border */
    width: 10px; /* Adjust the width as needed */
    height: 20px; /* Adjust the height as needed */
    margin: -7px 0; /* Center the handle */
    border-radius: 3px; /* Optional: round the corners */
}

QSlider::handle:vertical {
    background: #C8C8C8; /* Set the desired handle color */
    border: 1px solid rgba(0, 132, 185, 255); /* Optional: add a border */
    width: 20px; /* Adjust the width as needed */
    height: 10px; /* Adjust the height as needed */
    margin: 0 -7px; /* Center the handle */
    border-radius: 3px; /* Optional: round the corners */
}

QSlider::sub-page:horizontal {
    background: rgba(0, 132, 185, 255); /* Set the desired color for the left side of the gutter */
}

QSlider::sub-page:vertical {
    background: rgba(0, 132, 185, 255); /* Set the desired color for the top side of the gutter */
}

